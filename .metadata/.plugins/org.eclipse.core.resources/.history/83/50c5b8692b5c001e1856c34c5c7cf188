package demo;

public class Student {
	
	int rollno;
	
	String name;
	
	int age;
	
	Student(int i, String string, int j){
		this.rollno=rollno;
		this.name=name;
		this.age=age;
	}
	public static void main(String[] args) {
		
		Student s1=new Student(1, "Sahil", 22);
		System.out.println(s1);
	    Student s2=new Student(2, "Ravsaheb", 28);
	    System.out.println(s2);
	    
	}

}
